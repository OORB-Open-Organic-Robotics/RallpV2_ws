<?xml version="1.0" ?>

<robot name="RALLP_2">

<!-- Robot base -->

  <link name="base_link">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="0.8 0.59 0.25"/>
      </geometry>
      <material name="Cyan">
        <color rgba="0 1.0 1.0 1.0"/>
      </material>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="0.8 0.59 0.25"/>
      </geometry>
    </collision>
    <inertial>
      <origin xyz="0.0 0.0 0.0" rpy="0 0 0"/>
      <mass value="45"/>
      <inertia ixx="1.53975" ixy="0" ixz="0" 
              iyy="2.634375" iyz="0" izz="3.705375"/>
    </inertial>
  </link>



  <link name="base_footprint"/>
  <joint name="base_joint" type="fixed">
    <parent link="base_link"/>
    <child link="base_footprint"/>
    <origin rpy="0 0 0" xyz="0.0 0.0 -0.200853"/>
  </joint>
    


  <!-- Front Right Wheel -->

  <link name="fr_link">
    <visual>
      <origin xyz="0 -0.05 0" rpy="-1.570796 0.0 0.0"/>
      <geometry>
        <cylinder length="0.1" radius="0.125"/>
      </geometry>
      <material name="Gray">
        <color rgba="0.5 0.5 0.5 1.0"/>
      </material>
    </visual>
    <collision>
      <origin xyz="0 -0.05 0" rpy="-1.570796 0.0 0.0"/>
      <geometry>
        <cylinder length="0.1" radius="0.125"/>
      </geometry>
    </collision>
    <inertial>
      <origin xyz="-0.0 -0.05 0.0" rpy="0 0 0"/>
      <mass value="1.5"/>
      <inertia ixx="0.0096875" ixy="0" ixz="0" 
              iyy=" 0.01171" iyz="0" izz="0.0096875"/>
    </inertial>
  </link>
  <joint name="fr_joint" type="continuous">
    <parent link="base_link"/>
    <child link="fr_link"/>
    <origin xyz="0.2205 -0.295 -0.074147" rpy="0.0 0.0 0.0"/>
    <axis xyz="0 1 0"/>
     
  </joint>


  <!-- Front Left Wheel -->

  <link name="fl_link">
    <visual>
      <origin xyz="0 0.05 0" rpy="1.570796327 0.0 0"/>
      <geometry>
        <cylinder length="0.1" radius="0.125"/>
      </geometry>
      <material name="Gray">
        <color rgba="0.5 0.5 0.5 1.0"/>
      </material>
    </visual>
    <collision>
      <origin xyz="0 0.05 0" rpy="1.570796327 0.0 0.0"/>
      <geometry>
        <cylinder length="0.1" radius="0.125"/>
      </geometry>
    </collision>
    <inertial>
      <origin xyz="-0.0 0.05 0.0" rpy="0 0 0"/>
      <mass value="1.5"/>
      <inertia ixx="0.0096875" ixy="0" ixz="0" 
              iyy=" 0.01171" iyz="0" izz="0.0096875"/>
    </inertial>
  </link>
  <joint name="fl_joint" type="continuous">
    <parent link="base_link"/>
    <child link="fl_link"/>
    <origin xyz="0.2205 0.295 -0.074147" rpy="0 0.0 0.0"/>
    <axis xyz="0 1 0"/>
     
  </joint>


  <!-- Rear Left Wheel -->

  <link name="rl_link">
    <visual>
      <origin xyz="0 0.05 0" rpy="1.570796327 -0.0 -0.0"/>
      <geometry>
        <cylinder length="0.1" radius="0.125"/>
      </geometry>
      <material name="Gray">
        <color rgba="0.5 0.5 0.5 1.0"/>
      </material>
    </visual>
    <collision>
      <origin xyz="0 0.05 0" rpy="1.570796327 0 0"/>
      <geometry>
        <cylinder length="0.1" radius="0.125"/>
      </geometry>
    </collision>
    <inertial>
      <origin xyz="0 0.05 0" rpy="0 0 0"/>
      <mass value="1.5"/>
      <inertia ixx="0.0096875" ixy="0" ixz="0" 
              iyy=" 0.01171" iyz="0" izz="0.0096875"/>
    </inertial>
  </link>
  <joint name="rl_joint" type="continuous">
    <parent link="base_link"/>
    <child link="rl_link"/>
    <origin xyz="-0.2695 0.295 -0.074147" rpy="0 0.0 0.0"/>
    <axis xyz="0 1 0"/>
     
  </joint>


  <!-- Rear Right Wheel -->

  <link name="rr_link">
    <visual>
      <origin xyz="0 -0.05 0" rpy="-1.570796327 0.0 0.0"/>
      <geometry>
        <cylinder length="0.1" radius="0.125"/>
      </geometry>
      <material name="Gray">
        <color rgba="0.5 0.5 0.5 1.0"/>
      </material>
    </visual>
    <collision>
      <origin xyz="0 -0.05 0" rpy="-1.570796327 0.0 0.0"/>
      <geometry>
        <cylinder length="0.1" radius="0.125"/>
      </geometry>
    </collision>
    <inertial>
      <origin xyz="-0.0 -0.05 0.0" rpy="0 0 0"/>
      <mass value="1.5"/>
      <inertia ixx="0.0096875" ixy="0" ixz="0" 
              iyy=" 0.01171" iyz="0" izz="0.0096875"/>
    </inertial>
  </link>
  <joint name="rr_joint" type="continuous">
    <parent link="base_link"/>
    <child link="rr_link"/>
    <origin xyz="-0.2695 -0.295 -0.074147" rpy="6.283182245 0 0"/>
    <axis xyz="0 1 0"/>
     
  </joint>


  <!-- Lidar_link -->

  <link name="lidar_link">
    <visual>
      <geometry>
        <cylinder length="0.05" radius="0.06"/>
      </geometry>
      <material name="Red">
        <color rgba="1.0 0.0 0.0 1"/>
      </material>
    </visual>
    <collision>
      <geometry>
        <cylinder length="0.05" radius="0.06"/>
      </geometry>
    </collision>
  </link>
  <joint name="lidar_joint" type="fixed">
    <parent link="base_link"/>
    <child link="lidar_link"/>
    <origin rpy="0 0 0" xyz="0.15 0 0.15"/>
  </joint>

  <!-- imu_sensor -->

  <link name="imu_link">
    <visual>
      <geometry>
        <box size="0.01 0.01 0.01"/>
      </geometry>
      <material name="Red">
        <color rgba="1.0 1.0 0.0 1"/>
      </material>
    </visual>
    <inertial>
        <mass value="0.01"/>
        <inertia ixx="0.001" ixy="0.0" ixz="0.0" 
                 iyy="0.001" iyz="0.0" izz="0.001"/>
    </inertial>
    <collision>
      <geometry>
        <box size="0.01 0.01 0.01"/>
      </geometry>
    </collision>

  </link>
  <joint name="imu_joint" type="fixed">
    <parent link="base_link"/>
    <child link="imu_link"/>
    <origin rpy="0 0 0" xyz="0 0 0.05"/>
  </joint>


  <!-- Camera link -->

 <link name="camera_link">
   <visual>
     <origin xyz="0 0 0" rpy="0 0 0"/>
     <geometry>
       <box size="0.015 0.015 0.022"/>
     </geometry>
   </visual>
   <collision>
     <origin xyz="0 0 0" rpy="0 0 0"/>
     <geometry>
       <box size="0.01 0.05 0.05"/>
     </geometry>
   </collision>
   <inertial>
     <origin xyz="0 0 0" rpy="0 0 0"/>
     <mass value="0.035"/>
     <inertia ixx="0.00001" ixy="0" ixz="0" iyy="0.00001" iyz="0" izz="0.00001" />
   </inertial>
 </link>

 <joint name="camera_joint" type="fixed">
   <parent link="base_link"/>
   <child link="camera_link"/>
   <origin xyz="0.4 0 0" rpy="0 0 0"/>
 </joint>


  <!-- Gazebo plugin -->

  <gazebo>
    <plugin filename="gz-sim-diff-drive-system" name="gz::sim::systems::DiffDrive">
      <left_joint>fl_joint</left_joint>
      <left_joint>rl_joint</left_joint>
      <right_joint>fr_joint</right_joint>
      <right_joint>rr_joint</right_joint>
      <robotBaseFrame>base_link</robotBaseFrame>
      <wheel_separation>0.59</wheel_separation>
      <wheel_radius>0.125</wheel_radius>
      <odom_topic>odom</odom_topic>
      <frame_id>odom</frame_id>
      <child_frame_id>base_link</child_frame_id>
      <odom_publisher_frequency>50</odom_publisher_frequency>
      <wheel_acceleration>5.0</wheel_acceleration>  
      <wheel_torque>10.0</wheel_torque> 
      <tf_topic>/tf</tf_topic>
      <topic>cmd_vel</topic>
    </plugin>


    <!-- joint_states publisher -->

    <plugin filename="gz-sim-joint-state-publisher-system" name="gz::sim::systems::JointStatePublisher">
      <topic>joint_states</topic>
      <joint_name>fl_joint</joint_name>
      <joint_name>fr_joint</joint_name>
      <joint_name>rl_joint</joint_name>
      <joint_name>rr_joint</joint_name>
    </plugin> 


    <!-- Lidar plugin -->

    <plugin
      filename="gz-sim-sensors-system"
      name="gz::sim::systems::Sensors">
      <render_engine>ogre2</render_engine>
    </plugin>
  </gazebo>

  <gazebo reference="lidar_link">
    <sensor name="laser" type="gpu_lidar">
        <pose> 0 0 0 0 0 0 </pose>
        <visualize>true</visualize>
          <update_rate>10</update_rate>
         <lidar>
           <scan>
              <horizontal>
                <samples>1000</samples>
                <min_angle>-3.1415</min_angle>                   
                <max_angle>3.1415</max_angle>
              </horizontal>
           </scan>
          <range>
            <min>0.1</min>
            <max>12</max>
          </range>
         </lidar>
       <topic>scan</topic>
       <gz_frame_id>lidar_link</gz_frame_id>
    </sensor>
        
  </gazebo>


  <!-- Wheel friction Plugin-->

  <gazebo reference="fl_link">
     <mu1 value="1.5"/> 
     <mu2 value="  1.3"/>     
  </gazebo>

  <gazebo reference="rl_link">
     <mu1 value="1.5"/>  
     <mu2 value="  1.3"/>    
  </gazebo>

  <gazebo reference="rr_link">
     <mu1 value="1.5"/> 
     <mu2 value="  1.3"/>    
  </gazebo>

  <gazebo reference="fr_link">
    <mu1 value="1.5"/>  
    <mu2 value="  1.3"/>     
  </gazebo>

  <!--Simulation plugin-->
  <gazebo>
   <physics type="ode">
     <max_step_size>0.001</max_step_size>
     <real_time_factor>1</real_time_factor>
     <real_time_update_rate>1000</real_time_update_rate>
   </physics>
  </gazebo>
  
   <!-- IMU Plugin-->
  <gazebo reference="imu_link">
    <sensor name="imu_sensor" type="imu">
      <always_on>1</always_on>
      <update_rate>100</update_rate>
      <visualize>true</visualize>
      <topic>imu</topic>
    </sensor>
  </gazebo>
 
   <!-- Camera Plugin-->
  <gazebo reference="camera_link">
    <sensor name="camera_sensor" type="camera">
      <always_on>1</always_on>
      <update_rate>30</update_rate>
      <visualize>true</visualize>
      <camera>
        <horizontal_fov>1.39626</horizontal_fov>
        <image>
          <width>640</width>
          <height>480</height>
          <format>R8G8B8</format>
        </image>
        <clip>
          <near>0.1</near>
          <far>100</far>
        </clip>
      </camera>
      <topic>camera/image_raw</topic>
      <gz_frame_id>camera_link</gz_frame_id>
    </sensor>
  </gazebo>
</robot>